} 
else if(ptr1->expo > ptr2->expo) 
{ 
head3=insert(head3,ptr1->coeff,ptr1->expo); ptr1=ptr1-
>next; 
} 
else if(ptr1->expo < ptr2->expo) 
{ 
head3=insert(head3,ptr2->coeff,ptr2->expo); ptr2=ptr2-
>next; 
} 
} 
