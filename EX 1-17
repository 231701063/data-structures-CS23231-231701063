push(3); 
break; case 
'/': 
while (stack[top] >= 4) 
postfix[j++] = pop(); 
push(4); break; 
case '^': postfix[j++] = 
pop(); push(5); 
break; 
case '(': push(0); 
break; 
case ')': 
while (stack[top] != 0) 
postfix[j++] = pop(); 
top--; break; 
default: 
postfix[j++] = infix[i]; 
} 
} 
while (top > 0) 
postfix[j++] = pop(); 
printf("\nPostfix expression is =>\n\t\t%s", postfix); 
